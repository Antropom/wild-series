<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		{% include "_navbar.html.twig" %}
		<link rel="icon" href="{{ asset('favicon.ico')}}"/>
		<link rel="icon" type="image/png" href="{{ asset('build/favicon-96x96.png')}}"/>
	</head>
	<body>
		{% for category, messages in app.flashes %}
			{% for message in messages %}
				<div class="alert alert-{{ category }}">
					{{ message }}
				</div>
			{% endfor %}
		{% endfor %}
		{% block body %}{% endblock %}
	</body>
</html>
